{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFramework": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8080"
      },
      "Https": {
        "Url": "https://*:8443"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "../certs/self-signed.pem",
        "KeyPath": "../certs/self-signed.key"
      }
    }
  },
  "AllowedHosts": "*",
  "TelegramOptions": {
    "BotToken": "YOUR_BOT_TOKEN",
    "WebHookUrl": "https://YOUR_DOMAIN:443/api/telegram/update"
  },
  "ConnectionStrings": {
    "BenkyouDb": "YOUR_CONNECTION_STRING"
  } 
}
